+import sys
+import pygame
+
+from funcs import load_level, load_image
+from sprites import Tile, Object, all_sprites
+
+screen_size = width, height = (1280, 800)
+
+
+def tavern_screen(player):
+	from sprites import hero_group
+	running = True
+	screen = pygame.display.set_mode(screen_size)
+	screen.fill((0, 0, 0))
+	tavern = load_level('tavern.map')
+	from sprites import tile_width, tile_height
+	for y in range(len(tavern)):
+		for x in range(len(tavern[y])):
+			if tavern[y][x] == '.':
+				Tile(load_image('wood_boards.png'), x, y)
+			elif tavern[y][x] == '#':
+				Object(load_image('wood_boards.png'), x * tile_width, y * tile_height, (60, 60))
+
+	while running:
+		for event in pygame.event.get():
+			if event.type == pygame.QUIT:
+				running = False
+			elif event.type == pygame.MOUSEBUTTONDOWN:
+				player.location = 1
+				return
+		player.update()
+		hero_group.draw(screen)
+		all_sprites.custom_draw(player)
+		pygame.display.flip()
+	pygame.quit()
+	sys.exit()
